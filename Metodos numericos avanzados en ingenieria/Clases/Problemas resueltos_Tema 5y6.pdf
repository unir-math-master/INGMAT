    <script type="text/javascript">
    var siteUrl = '/';

    function showMessage(text, autoClose, fnClose) {
        var content = $('<div></div>').appendTo('body');
        content.append(
            '<div class="modal modal-show-message" tabindex="-1" role="dialog">' +
            '<div class="modal-dialog" role="document">' +
            '<div class="modal-content">' +
            '<div class="modal-body modal-message-body">' +
            '</div>' +
            '</div>' +
            '</div>' +
            '</div>');
        var body = $('.modal-message-body', content);
        body.append('<h5 class="text-center text-info center-block">' + text + '</h5>');

        $('.modal-show-message', content).on('hidden.bs.modal',
            function(e) {
                content.remove();
                if (fnClose) {
                    fnClose();
                }
            });

        $('.modal-show-message', content)
            .modal('show');

        if (autoClose && autoClose == true) {
            setTimeout(function() {
                    $('.modal-show-message', content).modal('hide');
                },
                3000);
        }
    }

    (function() {
        $('#consent_blackbar').hide();
        setTimeout(function() {
                if ($('#consent_blackbar *').length > 0) {
                    $('#consent_blackbar').show();
                }
            },
            3000);
    })();
    </script>
    
